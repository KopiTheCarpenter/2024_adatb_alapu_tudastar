<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tudastar.Controller.HibaController">
    <tabs>
        <Tab text="Read">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TableView fx:id="tv_hiba" layoutX="14.0" layoutY="14.0" prefHeight="310.0" prefWidth="972.0" />
                        <Button layoutX="14.0" layoutY="332.0" mnemonicParsing="false" onAction="#pressRead" text="Read" />
                    </children>
                </AnchorPane>
            </content>
        </Tab>
        <Tab text="Insert">
            <content>
                <GridPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="60" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="20" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Hiba Leírás" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                        <TextArea fx:id="insert_hiba_leiras" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Button fx:id="btn_insert" mnemonicParsing="false" onAction="#pressInsert" text="Insert" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                    </children>
                </GridPane>
            </content>
        </Tab>
        <Tab onSelectionChanged="#onSelectUpdate" text="Archive">
            <content>
                <GridPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <ComboBox fx:id="archive_combobox" prefWidth="150.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        <Button fx:id="btn_archive" mnemonicParsing="false" onAction="#pressArchive" text="Archive" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />

                    </children>
                </GridPane>
            </content>
        </Tab>
        <Tab onSelectionChanged="#onSelectDelete" text="Delete">
            <content>
                <GridPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <ComboBox fx:id="delete_combobox" prefWidth="150.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                        <Button fx:id="btn_delete" mnemonicParsing="false" onAction="#pressDelete" text="Delete" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />

                    </children>
                </GridPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
