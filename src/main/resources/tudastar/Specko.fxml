<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<TabPane xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         fx:controller="tudastar.Controller.SpeckoController"
         prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
    <tabs>
    <Tab text="Cikk/Szerzo">
        <content>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <children>
                    <TableView fx:id="tv_szerzo_cikk_count" layoutX="14.0" layoutY="14.0" prefHeight="310.0" prefWidth="972.0" />
                    <Button layoutX="14.0" layoutY="332.0" mnemonicParsing="false" onAction="#pressSzerzoCikkCount" text="Read" />
                </children>
            </AnchorPane>
        </content>
    </Tab>
        <Tab text="Lektor/nyelv">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TableView fx:id="tv_lektor_nyelv_count" layoutX="14.0" layoutY="14.0" prefHeight="310.0" prefWidth="972.0" />
                        <Button layoutX="14.0" layoutY="332.0" mnemonicParsing="false" onAction="#pressLektorLanguageCount" text="Read" />
                    </children>
                </AnchorPane>
            </content>
        </Tab>
        <Tab text="Felhasznalo/Hiba">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TableView fx:id="tv_felhasznalo_hiba_count" layoutX="14.0" layoutY="14.0" prefHeight="310.0" prefWidth="972.0" />
                        <Button layoutX="14.0" layoutY="332.0" mnemonicParsing="false" onAction="#pressFelhasznaloHibaCount" text="Read" />
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
